{
	"$schema": "https://esm.sh/wrangler/config-schema.json",
	"name": "tokenlist",
	"compatibility_date": "2025-12-17",
	"compatibility_flags": ["nodejs_compat"],
	"main": "./src/index.tsx",
	"keep_vars": true,
	"workers_dev": true,
	"preview_urls": true,
	"assets": {
		"directory": "data",
		"binding": "ASSETS",
		"run_worker_first": true
	},
	"observability": {
		"enabled": true,
		"logs": {
			"enabled": true,
			"persist": true,
			"head_sampling_rate": 1,
			"invocation_logs": true
		}
	},
	"r2_buckets": [
		{
			"bucket_name": "tokenlist",
			"binding": "TOKENLIST",
			"remote": true
		}
	]
}
