{
	"name": "contracts",
	"version": "0.0.1",
	"type": "module",
	"imports": {
		"#*": "./src/*",
		"#openapi.json": "./openapi.json",
		"#package.json": "./package.json"
	},
	"scripts": {
		"dev": "vite dev",
		"preview": "vite preview",
		"build": "vite build",
		"check": "biome check . --write --unsafe",
		"check:types": "tsgo --project tsconfig.json --noEmit",
		"gen:types": "test -f .env || cp .env.example .env; wrangler types",
		"postinstall": "pnpm gen:types",
		"tail": "wrangler tail",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "wrangler d1 migrations apply CONTRACTS-DB --local",
		"db:migrate:remote": "wrangler d1 migrations apply CONTRACTS-DB --remote",
		"db:studio": "drizzle-kit studio --config='drizzle.config.ts'",
		"db:push": "drizzle-kit push --config='drizzle.config.ts'"
	},
	"dependencies": {
		"@cloudflare/containers": "^0.0.31",
		"cbor-x": "^1.6.0",
		"drizzle-orm": "^0.45.1",
		"hono": "catalog:",
		"hono-rate-limiter": "catalog:",
		"ox": "catalog:",
		"semver": "^7.7.3",
		"tempo.ts": "catalog:",
		"viem": "catalog:"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@cloudflare/vite-plugin": "catalog:",
		"@cloudflare/workers-types": "catalog:",
		"@libsql/client": "^0.15.15",
		"@scalar/api-reference": "catalog:",
		"@total-typescript/ts-reset": "catalog:",
		"@types/bun": "^1.3.5",
		"@types/node": "catalog:",
		"@types/semver": "^7.7.1",
		"dbmate": "^2.28.0",
		"drizzle-kit": "^0.31.8",
		"typescript": "catalog:",
		"vite": "catalog:",
		"vite-plugin-devtools-json": "catalog:",
		"wrangler": "catalog:"
	},
	"license": "MIT"
}
