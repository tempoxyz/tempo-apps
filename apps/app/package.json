{
	"name": "app",
	"private": true,
	"type": "module",
	"imports": {
		"#*": "./src/*",
		"#wrangler.json": "./wrangler.jsonc"
	},
	"scripts": {
		"postinstall": "pnpm gen:types",
		"dev": "pnpm dev:presto",
		"dev:devnet": "CLOUDFLARE_ENV='devnet' vite dev --port 3001",
		"dev:moderato": "CLOUDFLARE_ENV='moderato' vite dev --port 3001",
		"dev:presto": "CLOUDFLARE_ENV='presto' vite dev --port 3001",
		"build": "vite build",
		"check": "pnpm check:biome && pnpm check:types",
		"check:biome": "biome check --write --unsafe",
		"check:types": "tsgo --project tsconfig.json --noEmit",
		"predeploy": "pnpm build",
		"deploy": "wrangler deploy",
		"gen:types": "test -f .env || cp .env.example .env; CLOUDFLARE_ENV= wrangler types",
		"preview": "vite preview --mode='preview'"
	},
	"dependencies": {
		"@emoji-mart/data": "catalog:",
		"@emoji-mart/react": "catalog:",
		"@tailwindcss/vite": "catalog:",
		"@tanstack/react-query": "catalog:",
		"@tanstack/react-router": "catalog:",
		"@tanstack/react-router-ssr-query": "catalog:",
		"@tanstack/react-start": "catalog:",
		"@tanstack/router-plugin": "catalog:",
		"@wagmi/core": "catalog:",
		"animejs": "catalog:",
		"i18next": "^25.2.1",
		"idxs": "^0.0.5",
		"ox": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"react-i18next": "^15.5.3",
		"tailwindcss": "catalog:",
		"uqr": "^0.1.2",
		"viem": "catalog:",
		"wagmi": "catalog:",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "catalog:",
		"@iconify/json": "catalog:",
		"@svgr/core": "catalog:",
		"@svgr/plugin-jsx": "catalog:",
		"@tanstack/devtools-vite": "catalog:",
		"@tanstack/react-devtools": "catalog:",
		"@tanstack/react-query-devtools": "catalog:",
		"@tanstack/react-router-devtools": "catalog:",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/animejs": "catalog:",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@vitejs/plugin-react": "catalog:",
		"jsonc-parser": "catalog:",
		"tw-animate-css": "catalog:",
		"typed-query-selector": "catalog:",
		"unplugin-icons": "catalog:",
		"vite": "catalog:",
		"wrangler": "catalog:"
	}
}
