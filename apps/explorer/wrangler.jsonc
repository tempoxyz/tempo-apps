{
	"$schema": "https://esm.sh/wrangler/config-schema.json",
	"compatibility_date": "2025-12-17",
	"compatibility_flags": ["nodejs_compat"],
	"main": "./src/index.server.ts",
	"keep_vars": true,
	"workers_dev": true,
	"preview_urls": true,
	"placement": { "mode": "smart" },
	// this binding is not inheritable to environments
	// it's being added at top-level solely for the type generation script
	"browser": { "binding": "BROWSER" },
	"env": {
		"testnet": {
			"name": "testnet-explorer",
			"routes": [
				{
					"custom_domain": true,
					"zone_name": "tempo.xyz",
					"pattern": "explore.tempo.xyz"
				}
			],
			"vars": {
				"VITE_TEMPO_ENV": "testnet",
				"VITE_TEMPO_RPC_WS": "wss://proxy.tempo.xyz/rpc",
				"VITE_TEMPO_RPC_HTTP": "https://proxy.tempo.xyz/rpc"
			},
			"workers_dev": true,
			"browser": { "binding": "BROWSER" },
			"observability": {
				"enabled": true,
				"logs": {
					"persist": true,
					"enabled": true,
					"head_sampling_rate": 1,
					"invocation_logs": true
				}
			}
		},
		"devnet": {
			"name": "devnet-explorer",
			"routes": [
				{
					"custom_domain": true,
					"zone_name": "tempo.xyz",
					"pattern": "devnet.explore.tempo.xyz"
				}
			],
			"vars": {
				"VITE_TEMPO_ENV": "devnet",
				"VITE_TEMPO_RPC_WS": "wss://rpc.devnet.tempoxyz.dev",
				"VITE_TEMPO_RPC_HTTP": "https://rpc.devnet.tempoxyz.dev"
			},
			"workers_dev": true,
			"browser": { "binding": "BROWSER" },
			"observability": {
				"enabled": true,
				"logs": {
					"persist": true,
					"enabled": true,
					"head_sampling_rate": 1,
					"invocation_logs": true
				}
			}
		}
	},
	"observability": {
		"enabled": true,
		"logs": {
			"enabled": true,
			"head_sampling_rate": 1,
			"invocation_logs": true,
			"persist": true
		}
	}
}
