{
	"name": "@tempo/apps",
	"private": true,
	"type": "module",
	"workspaces": [
		"apps/*"
	],
	"scripts": {
		"build": "bun run --filter=* build",
		"deploy": "bun run --filter=* deploy",
		"dev": "bun run --filter=* dev",
		"check": "biome check --write --unsafe",
		"check:types": "bun run --filter=* check:types",
		"gen:types": "bun run --filter=* gen:types",
		"postinstall": "bun run --filter=* postinstall",
		"preview": "bun run --filter=* preview"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.4",
		"typescript": "^5.9.3"
	},
	"packageManager": "bun@1.3.2"
}
