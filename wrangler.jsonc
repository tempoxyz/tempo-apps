{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "tempo-app",
	"compatibility_date": "2025-11-01",
	"compatibility_flags": ["nodejs_compat"],
	"main": "@tanstack/react-start/server-entry",
	"browser": {
		"binding": "BROWSER"
	},
	"observability": {
		"enabled": true,
		"logs": {
			"enabled": true,
			"head_sampling_rate": 1,
			"invocation_logs": true,
			"persist": true
		}
	}
}
